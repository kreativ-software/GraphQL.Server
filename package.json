  "bugs": {
    "url": "https://github.com/LobsterInk/GraphQL.Server/GraphQL.Server/issues"
  },
  "homepage": "https://github.com/LobsterInk/GraphQL.Server/GraphQL.Server",
  "dependencies": {
  },
  "devDependencies": {
    "babel": "^5.6.14",
    "chalk": "^1.1.3",
    "child-process-promise": "^1.1.0",
    "moment": "^2.13.0",
    "rimraf": "^2.5.2",
    "shelljs": "^0.7.4",
    "superagent": "^2.0.0",
    "xmlbuilder": "^8.2.2"
  }
